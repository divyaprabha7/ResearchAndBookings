<?xml version="1.0" encoding="UTF-8"?>
<database>
    <element label="Reservation" max_length="40" name="x_snc_randb_reservation" type="collection">
        <element label="Charge code" max_length="32" name="charge_code" reference="x_snc_randb_charge_code" type="reference"/>
        <element label="Date" max_length="40" name="date" type="glide_date"/>
        <element label="Equipment" max_length="32" name="equipment" reference="x_snc_randb_equipment" reference_qual="javascript:new ResearchAndBookings().getEquipmentsforReservation(current.equipment_type,current.date,current.time_slot,current.location)" type="reference" use_reference_qualifier="advanced"/>
        <element label="Equipment type" max_length="32" name="equipment_type" reference="x_snc_randb_equipment_type" type="reference"/>
        <element label="Location" max_length="32" name="location" reference="cmn_location" reference_qual="javascript:new ResearchAndBookings().getLocationsforReservation(current.equipment_type,current.date,current.time_slot)" type="reference" use_reference_qualifier="advanced"/>
        <element attributes="ignore_filter_on_new=true" default="javascript:global.getNextObjNumberPadded();" label="Number" max_length="40" name="number" type="string"/>
        <element label="Reason for requesting" max_length="500" name="reason_for_requesting" type="string"/>
        <element choice="1" default="Draft" label="Status" max_length="40" name="status" type="choice">
            <choice>
                <element label="Draft" sequence="0" value="Draft"/>
                <element label="Requested for Approval" sequence="1" value="Requested for Approval"/>
                <element label="Approved" sequence="2" value="Approved"/>
                <element label="Rejected" sequence="3" value="Rejected"/>
                <element label="Cancelled" sequence="4" value="Cancelled"/>
            </choice>
        </element>
        <element label="Time slot" max_length="32" name="time_slot" reference="x_snc_randb_schedule_template_slot" type="reference"/>
        <element choice="1" label="Training required" max_length="40" name="training_required" type="choice">
            <choice>
                <element label="Yes" sequence="0" value="Yes"/>
                <element label="No" sequence="1" value="No"/>
            </choice>
        </element>
        <element label="Work notes" max_length="4000" name="work_notes" type="journal"/>
        <index name="index">
            <element name="charge_code"/>
        </index>
        <index name="index2">
            <element name="equipment"/>
        </index>
        <index name="index3">
            <element name="location"/>
        </index>
        <index name="index4">
            <element name="equipment_type"/>
        </index>
        <index name="index5">
            <element name="time_slot"/>
        </index>
    </element>
</database>
