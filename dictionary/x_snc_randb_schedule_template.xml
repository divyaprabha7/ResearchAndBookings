<?xml version="1.0" encoding="UTF-8"?>
<database>
    <element label="Schedule Template" max_length="40" name="x_snc_randb_schedule_template" type="collection">
        <element default="true" label="Active" max_length="40" name="active" type="boolean"/>
        <element label="Days to schedule" max_length="40" name="days_to_schedule" type="integer"/>
        <element label="Duration" max_length="100" name="duration" type="string"/>
        <element label="End time" max_length="40" name="end_time" type="glide_time"/>
        <element label="Friday" max_length="40" name="friday" type="boolean"/>
        <element label="Monday" max_length="40" name="monday" type="boolean"/>
        <element choice="3" default="1" label="Reservation length" max_length="100" name="reservation_length" type="choice">
            <choice>
                <element label="1 hrs" sequence="0" value="1 hrs"/>
                <element label="2 hrs" sequence="1" value="2 hrs"/>
                <element label="3 hrs" sequence="2" value="3 hrs"/>
                <element label="4 hrs" sequence="3" value="4 hrs"/>
                <element label="5 hrs" sequence="4" value="5 hrs"/>
                <element label="6 hrs" sequence="5" value="6 hrs"/>
                <element label="7 hrs" sequence="6" value="7 hrs"/>
                <element label="8 hrs" sequence="7" value="8 hrs"/>
                <element label="9 hrs" sequence="8" value="9 hrs"/>
                <element label="10 hrs" sequence="9" value="10 hrs"/>
                <element label="11 hrs" sequence="10" value="11 hrs"/>
                <element label="12 hrs" sequence="11" value="12 hrs"/>
            </choice>
        </element>
        <element label="Saturday" max_length="40" name="saturday" type="boolean"/>
        <element label="Start time" max_length="40" name="start_time" type="glide_time"/>
        <element label="Sunday" max_length="40" name="sunday" type="boolean"/>
        <element label="Thursday" max_length="40" name="thursday" type="boolean"/>
        <element display="true" label="Title" max_length="200" name="title" type="string"/>
        <element label="Tuesday" max_length="40" name="tuesday" type="boolean"/>
        <element label="Wednesday" max_length="40" name="wednesday" type="boolean"/>
    </element>
</database>
